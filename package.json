{
  "name": "unfake",
  "version": "0.1.0",
  "description": "utilities of hezulong1",
  "author": "hezulong1",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "build": "rollup -c",
    "release": "node ./scripts/release.js",
    "test": "vitest",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "pub": "npm publish --no-git-checks",
    "unpub": "npm unpublish unfake@0.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/hezulong1/unfake.git"
  },
  "bugs": {
    "url": "https://github.com/hezulong1/unfake/issues"
  },
  "keywords": [
    "util"
  ],
  "sideEffects": false,
  "main": "out/shared.cjs.js",
  "module": "out/shared.esm.js",
  "types": "out/shared.d.ts",
  "exports": {
    ".": {
      "types": "./out/shared.d.ts",
      "import": "./out/shared.esm.js",
      "require": "./out/shared.cjs.js"
    },
    "./*": {
      "types": "./out/*.d.ts",
      "import": "./out/*.esm.js",
      "require": "./out/*.cjs.js"
    }
  },
  "files": [
    "out",
    "src",
    "eslint-rules",
    "tsconfig.base.json"
  ],
  "engines": {
    "node": ">=16.0.0"
  },
  "devDependencies": {
    "@rollup/plugin-alias": "^5.1.0",
    "@vueuse/core": "^10.10.0",
    "conventional-changelog-cli": "^4.0.0",
    "esbuild": "^0.18.10",
    "eslint": "^8.48.0",
    "eslint-config-h21": "^0.15.0",
    "minimist": "^1.2.8",
    "rollup": "^3.27.1",
    "rollup-plugin-dts": "^6.0.1",
    "rollup-plugin-esbuild": "^5.0.0",
    "typescript": "~5.2.2",
    "vite": "4.5.3",
    "vitest": "0.29.8",
    "vue": "^3.3.4"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  }
}
