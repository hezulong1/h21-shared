{
  "name": "h21-shared",
  "version": "0.0.0",
  "description": "utilities of hezulong1",
  "author": "hezulong1<https://github.com/hezulong1>",
  "license": "MIT",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/hezulong1/h21-shared.git"
  },
  "bugs": {
    "url": "https://github.com/hezulong1/h21-shared/issues"
  },
  "keywords": [
    "h21"
  ],
  "sideEffects": false,
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.esm.js",
      "require": "./dist/index.cjs.js"
    },
    "./tsconfig": "./config/tsconfig.base.json",
    "./eslint-rules": "./eslint-rules/index.cjs",
    "./resource/*": "./resource/*",
    "./*": {
      "types": "./dist/*.d.ts",
      "import": "./dist/*.esm.js",
      "require": "./dist/*.cjs.js"
    }
  },
  "files": [
    "config",
    "dist",
    "eslint-rules",
    "resource",
    "src"
  ],
  "engines": {
    "node": ">=16"
  },
  "scripts": {
    "build": "rollup -c",
    "release": "node ./scripts/release.mjs",
    "test": "vitest",
    "lint": "eslint .",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s"
  },
  "devDependencies": {
    "@rollup/plugin-alias": "^5.1.0",
    "@vueuse/core": "^10.10.0",
    "conventional-changelog-cli": "^4.0.0",
    "esbuild": "^0.18.10",
    "eslint": "^8.48.0",
    "eslint-config-h21": "^0.15.0",
    "minimist": "^1.2.8",
    "rollup": "^3.27.1",
    "rollup-plugin-dts": "^6.0.1",
    "rollup-plugin-esbuild": "^5.0.0",
    "typescript": "~5.2.2",
    "vite": "4.5.3",
    "vitest": "0.29.8",
    "vue": "^3.3.4"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  }
}
