{
  "npm.packageManager": "yarn",
  "search.exclude": {
    "**/dist": true,
    "**/out": true
  },
  "typescript.tsdk": "node_modules/typescript/lib",
  "[vue]": {
    "editor.defaultFormatter": "Vue.volar",
    "editor.codeActionsOnSave": {
      "source.fixAll.eslint": "explicit",
      "source.fixAll.stylelint": "explicit"
    }
  }
}
