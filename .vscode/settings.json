{
  "npm.packageManager": "yarn",
  "search.exclude": {
    "**/dist": true,
    "**/dist-electron": true,
    "**/release": true
  },
  "[vue]": {
    "editor.defaultFormatter": "Vue.volar",
    "editor.codeActionsOnSave": {
      "source.fixAll.eslint": "explicit",
      "source.fixAll.stylelint": "explicit"
    },
  }
}
